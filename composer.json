{
    "name": "simpod/json-rpc",
    "description": "PHP JSON-RPC 2.0 PSR-7 Message factory",
    "keywords": ["http", "json", "json-rpc", "jsonrpc", "ring", "rpc"],
    "homepage": "https://github.com/simpod/jsonrpc",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "SimPod\\JsonRpc\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimPod\\JsonRpc\\tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "nyholm/psr7": "^1.2",
        "php-http/httplug": "^2.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "thecodingmachine/safe": "^2"
    },
    "require-dev": {
        "cdn77/coding-standard": "^6.0",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^1.0.0",
        "phpstan/phpstan-phpunit": "^1.0.0",
        "phpstan/phpstan-strict-rules": "^1.0.0",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "0.17.0",
        "thecodingmachine/phpstan-safe-rule": "^1.0",
        "vimeo/psalm": "4.25.0"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    }
}
